{
  "name": "react-boiler-plate",
  "version": "1.0.0",
  "description": "react boiler plate",
  "main": "index.js",
  "engine": {
    "node": "14.15.0",
    "npm": "6.14.8"
  },
  "scripts": {
    "start": "node server/index.js",
    "backend": "nodemon server/index.js",
    "frontend": "npm run front --prefix client",
    "dev": "concurrently \"npm run backend\" \"npm run start --prefix client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "swacky",
  "license": "ISC",
  "dependencies": {
    "async": "^3.2.0",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dialogflow": "^4.0.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.11.8",
    "multer": "^1.4.2",
    "react-helmet": "^6.1.0",
    "react-redux": "^5.1.2",
    "react-simple-chatbot": "^0.6.1",
    "socket.io": "^2.3.0",
    "styled-components": "^5.2.1",
    "typekit": "^0.1.1"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.3.0",
    "concurrently": "^5.3.0",
    "nodemon": "^1.19.4"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
